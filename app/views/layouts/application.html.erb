<!DOCTYPE html>
<html>
  <head>
    <title>LloydsPayroll</title>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
    <% if current_user %>
      <%= render 'shared/nav' %>
    <% end %>
    <%= alerts %> 
    <div class="container-fluid">
      <%= yield %>
    </div>
  </body>
</html>
